<h1>Sellers</h1>

<a href="/sellers/new">+ Add New Seller</a>

<div class="grid">
  <% for seller in @sellers %>
  <div class="grid-row">
    <div class="cell">
      <p><%= seller.name %></p>
    </div>
    <div class="cell">
      <a href="/sellers/edit/<%= seller.id %>">Update Seller Details</a>
    </div>
    <div class="cell">
      <a href="/sellers/delete/<%= seller.id %>">Remove Seller</a>
    </div>
  </div>
  <% end %>
</div>
